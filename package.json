{
    "name": "vscode-angular2-files",
    "displayName": "Angular2 Files",
    "description": "Quickly angular file templates",
    "version": "1.0.0",
    "icon": "images/icon.png",
    "publisher": "fedorax",
    "author": {
        "name": "fedorax"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fedorax/vscode-angular2-files"
    },
    "bugs": {
        "url": "https://github.com/fedorax/vscode-angular2-files/issues"
    },
    "keywords": [
        "Angular 2",
        "angular-cli",
        "angular",
        "TypeScript",
        "Templates"
    ],
    "categories": [
        "Other",
        "Languages",
        "Snippets"
    ],
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "activationEvents": [
        "workspaceContains:package.json"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
                "command": "extension.addAngular2Component",
                "title": "Generate Component"
            },
            {
                "command": "extension.addAngular2Directive",
                "title": "Generate Directive"
            },
            {
                "command": "extension.addAngular2Pipe",
                "title": "Generate Pipe"
            },
            {
                "command": "extension.addAngular2Service",
                "title": "Generate Service"
            },
            {
                "command": "extension.addAngular2Class",
                "title": "Generate Class"
            },
            {
                "command": "extension.addAngular2Interface",
                "title": "Generate Interface"
            },
            {
                "command": "extension.addAngular2Route",
                "title": "Generate Route"
            },
            {
                "command": "extension.addAngular2Enum",
                "title": "Generate Enum"
            },
            {
                "command": "extension.addAngular2Module",
                "title": "Generate Module"
            }
        ],
        "menus": {
            "explorer/context": [{
                    "when": "",
                    "command": "extension.addAngular2Component",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Directive",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Pipe",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Service",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Module",
                    "group": "Angular2"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Class",
                    "group": "Angular2 Essentials"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Interface",
                    "group": "Angular2 Essentials"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Route",
                    "group": "Angular2 Essentials"
                },
                {
                    "when": "",
                    "command": "extension.addAngular2Enum",
                    "group": "Angular2 Essentials"
                }
            ]
        }
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "rimraf": "^2.4.4",
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "q": "^1.4.1"
    }
}